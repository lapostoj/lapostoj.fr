{
  "version": "0.2.0",
  "name": "lapostoj-website",
  "description": "CV and Portfolio and Blog website for lapostoj",
  "main": "n/a",
  "scripts": {
    "build": "npm run clean:public && gatsby build",
    "clean:public": "rm -rf public && rm -rf .cache",
    "clean": "npm run clean:public",
    "deploy": "firebase deploy --token $FIREBASE_TOKEN",
    "lint": "eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "lint:markdown": "markdownlint 'src/**/*.md'",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "start": "npm run clean:public && gatsby develop",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "husky install"
  },
  "repository": "git+https://github.com/lapostoj/lapostoj.fr.git",
  "keywords": [
    "gatsby",
    "react",
    "lapostoj",
    "blog",
    "cv",
    "portfolio",
    "resume"
  ],
  "author": "lapostoj <jerome.lapostolet@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "fix": "^0.0.6",
    "gatsby": "^4.23.1",
    "gatsby-plugin-catch-links": "^4.23.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^4.23.1",
    "gatsby-plugin-offline": "^5.23.1",
    "gatsby-plugin-react-helmet": "^5.23.0",
    "gatsby-plugin-sass": "^5.23.1",
    "gatsby-plugin-sharp": "^4.23.1",
    "gatsby-plugin-sitemap": "^5.23.1",
    "gatsby-remark-autolink-headers": "^5.23.1",
    "gatsby-remark-copy-linked-files": "^5.21.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^6.23.1",
    "gatsby-remark-prismjs": "^6.22.0",
    "gatsby-remark-responsive-iframe": "^5.23.0",
    "gatsby-remark-smartypants": "^5.23.0",
    "gatsby-source-filesystem": "^4.23.0",
    "gatsby-transformer-remark": "^5.23.1",
    "gatsby-transformer-sharp": "^4.23.1",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --cache --ignore-pattern public"
    ],
    "src/**/*.md": [
      "markdownlint"
    ]
  },
  "browserslist": [
    "defaults",
    "not ie <= 11",
    "not op_mini all"
  ]
}
